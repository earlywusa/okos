<p id="notice"><%= notice %></p>

<h1 class='center'>Practitioners</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Title</th>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Clinic Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @practitioners.each do |practitioner| %>
      <tr>
        <td><%= practitioner.title %></td>
        <td><%= practitioner.first_name %></td>
        <td><%= practitioner.last_name %></td>
        <td><%= practitioner.clinic_name %></td>
        <td><%= link_to 'Details', practitioner %></td>
        <!-- <td><%= link_to 'Edit', edit_practitioner_path(practitioner) %></td>
        <td><%= link_to 'Destroy', practitioner, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>

<br>
